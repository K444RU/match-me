{
  "name": "match-me-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "release": "commit-and-tag-version",
    "server": "json-server --watch src/db.json --port 8000",
    "gen:types": "orval --config ./orval.config.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "cd .. && husky front-end/.husky",
    "preinstall": "npx only-allow pnpm",
    "test": "playwright test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@reduxjs/toolkit": "^2.7.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-virtual": "^3.13.6",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "motion": "^11.18.2",
    "multi-range-slider-react": "^2.0.7",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-avatar-editor": "^13.0.2",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "react-phone-number-input": "^3.4.12",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.0",
    "react-stomp-hooks": "^3.0.1",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@faker-js/faker": "^9.7.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.20",
    "@types/react-avatar-editor": "^13.0.4",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.0",
    "commit-and-tag-version": "^12.5.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "json-server": "1.0.0-beta.3",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.4",
    "orval": "^7.8.0",
    "postcss": "^8.5.3",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw"
    ],
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint-plugin-tailwindcss>tailwindcss": "4",
        "react-avatar-editor>react": "^19.1.0",
        "react-avatar-editor>react-dom": "^19.1.0"
      }
    }
  }
}
