{
  "include-component-in-tag": true,
  "include-v-in-tag": false,
  "separate-pull-requests": true,
  "packages": {
    "front-end": {
      "component": "front-end",
      "release-type": "node",
      "path": "front-end",
      "package-name": "match-me-frontend"
    },
    "back-end": {
      "component": "back-end",
      "release-type": "maven",
      "path": "back-end",
      "package-name": "srv",
      "handleSnapshotRelease": "maven-snapshot",
      "snapshotLabel": "SNAPSHOT"
    }
  }
}
