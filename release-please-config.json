{
  "include-component-in-tag": true,
  "include-v-in-tag": false,
  "separate-pull-requests": true,
  "packages": {
    "front-end": {
      "component": "front-end",
      "release-type": "node",
      "path": "front-end",
      "package-name": "match-me-frontend",
      "changelog-path": "front-end/CHANGELOG.md"
    },
    "back-end": {
      "component": "back-end",
      "release-type": "maven",
      "path": "back-end",
      "package-name": "srv",
      "changelog-path": "back-end/CHANGELOG.md",
      "extra-files": [
        {
          "type": "json",
          "path": ".release-please-manifest.json",
          "jsonpath": "$.back-end"
        }
      ]
    }
  }
}
